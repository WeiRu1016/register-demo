
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "React": true,
    "ReactDOM": true,
    "document": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "jsx-a11y/label-has-for": 0
  }
}