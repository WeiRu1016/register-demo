{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9",
          "Android >= 4",
          "iOS >= 8",
          "chrome >= 40",
          "safari >= 8"
        ]
      },
      "debug": false,
      "useBuiltIns": true
    }],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime"
  ]
}
